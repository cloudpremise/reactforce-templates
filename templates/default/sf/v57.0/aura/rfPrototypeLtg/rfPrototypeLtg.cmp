<aura:component controller="rfPrototypeLtgCtrl" implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,forceCommunity:availableForAllPageTypes,lightning:isUrlAddressable,force:hasRecordId" access="global">
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:attribute name="page" type="String" default="contacts" />
    <aura:attribute name="bundleDomain" type="String" />
    <aura:attribute name="advancedConfiguration" type="Boolean" default="true" />
    <aura:attribute name="userInterfacePreview" type="Boolean" default="true" />
    <aura:attribute access="global" name="hidePageHeader" type="Boolean" default="false" />
    <aura:attribute access="global" name="hideComponentHeader" type="Boolean" default="false" />
    <aura:attribute access="global" name="hideNavigationTabs" type="Boolean" default="false" />
    <aura:attribute name="containerHeight" type="String" default="600" />

    <div>
        <div style="{!'height:'+v.containerHeight+'px'}">
            {!v.body}
        </div>
    </div>
</aura:component>